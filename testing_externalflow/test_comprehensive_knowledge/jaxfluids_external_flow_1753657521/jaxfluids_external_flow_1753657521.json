{
  "case_name": "comprehensive_3d_external_flow_test",
  "description": "A comprehensive 3D simulation of subsonic, viscous external flow over a generic immersed boundary, configured as a wind tunnel test.",
  "domain": {
    "x": {
      "cells": 256,
      "range": [
        -15300.0,
        17000.0
      ]
    },
    "y": {
      "cells": 192,
      "range": [
        -11900.0,
        11900.0
      ]
    },
    "z": {
      "cells": 192,
      "range": [
        -11900.0,
        11900.0
      ]
    },
    "decomposition": {
      "split_x": 1,
      "split_y": 1,
      "split_z": 1
    }
  },
  "boundary_conditions": {
    "west": {
      "type": "DIRICHLET",
      "primitives_callable": {
        "rho": 1.225,
        "u": 100.0,
        "v": 0.0,
        "w": 0.0,
        "p": 101325.0
      }
    },
    "east": {
      "type": "ZEROGRADIENT"
    },
    "north": {
      "type": "SYMMETRY"
    },
    "south": {
      "type": "SYMMETRY"
    },
    "top": {
      "type": "SYMMETRY"
    },
    "bottom": {
      "type": "SYMMETRY"
    }
  },
  "material_properties": {
    "equation_of_state": {
      "model": "IdealGas",
      "specific_heat_ratio": 1.4,
      "specific_gas_constant": 287.0
    },
    "transport": {
      "dynamic_viscosity": {
        "model": "CUSTOM",
        "value": 1.8e-05
      },
      "bulk_viscosity": 0.0,
      "thermal_conductivity": {
        "model": "CUSTOM",
        "value": 0.024
      }
    }
  },
  "initial_condition": {
    "primitives": {
      "rho": 1.225,
      "u": 100.0,
      "v": 0.0,
      "w": 0.0,
      "p": 101325.0
    },
    "levelset": "custom_sdf"
  },
  "immersed_boundary": {
    "type": "WALL"
  },
  "time_integration": {
    "method": "RK3"
  },
  "time_stepping": {
    "cfl_number": 0.4,
    "iterations": 100
  },
  "solver": {
    "flux_scheme": "AUSM",
    "reconstruction": {
      "scheme": "MINMOD"
    }
  },
  "forcings": {
    "gravity": [
      0.0,
      0.0,
      0.0
    ]
  },
  "output": {
    "primitives": [
      "density",
      "velocity",
      "pressure",
      "temperature"
    ],
    "levelset": [
      "volume_fraction",
      "levelset",
      "normal"
    ],
    "miscellaneous": [
      "mach_number",
      "schlieren",
      "vorticity",
      "kinetic_energy"
    ],
    "output_frequency": 25,
    "output_path": "./output/"
  }
}